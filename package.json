{
    "title": "SIGMun Hero Package",
    "name": "sigmun-hero",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "npm run clean && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "Pacote de componentes para a aplicação SIGMun",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "license": "MIT",
    "author": "Heronildes Junior",
    "contributors": [
        "David Miller (https://davidmiller.io/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/heronildesjr/sigmun-hero.git"
    },
    "dependencies": {
        "bootstrap": "^5.1.3",
        "bootstrap-icons": "^1.9.1"
    },
    "devDependencies": {
        "autoprefixer": "10.4.4",
        "browser-sync": "^2.29.1",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "postcss": "8.4.12",
        "prettier": "2.6.0",
        "pug": "3.0.2",
        "sass": "1.49.9",
        "shelljs": "0.8.5",
        "upath": "2.0.1"
    },
    "exports": {
        "main": "./dist/sigmun.js",
        "style": "./dist/sigmun-default.css",
        "files": [
            "./dist"
        ],
        "types": "./dist/sigmun.d.ts",
        "default": "./dist/sigmun.js"
    }
}
